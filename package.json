{
    "name": "react-material-table",
    "version": "0.0.1",
    "description": "Table component for React with a material-ui feel",
    "main": "index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack-dev-server  --mode development --inline --hot --open",
