        "react-dom": "^16.8.0-alpha.0"
    },
    "devDependencies": {
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "file-loader": "^3.0.1",
