{
    "name": "react-material-table",
    "version": "0.0.1",
    "description": "Table component for React with a material-ui feel",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "scripts": {
        "test": "jest",
        "clean": "rm -rf ./dist && mkdir dist",
        "build": "npm run clean && cross-env NODE_ENV=production rollup --config ",
        "format": "tslint --fix \"src/**.{ts,tsx}\" --project tsconfig.json",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "updateJest": "jest --updateSnapshot",
        "watch": "cross-env NODE_ENV=development rollup -cw"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm test"
        }
    },
    "lint-staged": {
        "src/**.{ts,tsx}": [
            "tslint --project tsconfig.json"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git@github.ibm.com:nwaywood/react-material-table.git"
    },
    "keywords": [
        "react"
    ],
    "author": "Nick Waywood, Antony Targett",
    "license": "ISC",
    "dependencies": {
        "@emotion/core": "^10.0.6",
        "@emotion/styled": "^10.0.6",
        "react": "^16.8.0-alpha.0",
        "react-dom": "^16.8.0-alpha.0"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@types/jest": "^23.3.13",
        "@types/react": "^16.7.18",
        "@types/react-dom": "^16.0.11",
        "babel-plugin-annotate-pure-calls": "^0.4.0",
        "babel-plugin-dev-expression": "^0.2.1",
        "babel-plugin-transform-rename-import": "^2.3.0",
        "cross-env": "^5.2.0",
        "file-loader": "^3.0.1",
        "husky": "^1.3.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.6.0",
        "lint-staged": "^8.1.0",
        "prettier": "^1.15.3",
        "react-testing-library": "^5.4.4",
        "rollup": "^1.1.1",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.2.0",
        "rollup-plugin-fill-html": "^1.1.0",
        "rollup-plugin-livereload": "^0.6.0",
        "rollup-plugin-node-resolve": "^4.0.0",
        "rollup-plugin-replace": "^2.1.0",
        "rollup-plugin-serve": "^1.0.0",
        "rollup-plugin-size-snapshot": "^0.8.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-svg": "^1.0.1",
        "rollup-plugin-typescript": "^1.0.0",
        "ts-jest": "^23.10.5",
        "tslib": "^1.9.3",
        "tslint": "^5.12.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.17.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-plugin-prettier": "^2.0.1",
        "tslint-react": "^3.6.0",
        "typescript": "^3.2.2"
    }
}
